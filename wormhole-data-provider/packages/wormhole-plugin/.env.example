# Wormholescan API Configuration
# Copy this file to .env and fill in your values

# Wormholescan API doesn't require an API key (public API)
# You can leave this empty or set any value
WORMHOLE_API_KEY=not-required

# Wormholescan API Base URL
# Default: https://api.wormholescan.io/api/v1
WORMHOLE_BASE_URL=https://api.wormholescan.io/api/v1

# Request timeout in milliseconds
# Default: 10000 (10 seconds)
# Min: 1000, Max: 60000
WORMHOLE_TIMEOUT=10000

# Maximum requests per second for rate limiting
# Default: 10
WORMHOLE_REQUESTS_PER_SECOND=10

# Maximum number of retry attempts for failed requests
# Default: 3
WORMHOLE_MAX_RETRIES=3
