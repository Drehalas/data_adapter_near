# Wormhole API Configuration
# Copy this file to .env and fill in your values

# Optional: Wormhole API Key (Bearer token)
# The plugin works without an API key but may have lower rate limits
WORMHOLE_API_KEY=

# Wormhole API Base URL
# Default: https://api.wormhole.com
WORMHOLE_BASE_URL=https://api.wormhole.com

# Request timeout in milliseconds
# Default: 10000 (10 seconds)
# Min: 1000, Max: 60000
WORMHOLE_TIMEOUT=10000

# Maximum requests per second for rate limiting
# Default: 10
WORMHOLE_REQUESTS_PER_SECOND=10

# Maximum number of retry attempts for failed requests
# Default: 3
WORMHOLE_MAX_RETRIES=3
